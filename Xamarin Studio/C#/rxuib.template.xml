<?xml version="1.0" encoding="utf-8"?>
<CodeTemplates version="3.0">
  <CodeTemplate version="2.0">
    <Header>
      <_Group>C#</_Group>
      <Version />
      <MimeType>text/x-csharp</MimeType>
      <Shortcut>rxuib</Shortcut>
      <_Description>Inserts an RxUI Bind</_Description>
      <TemplateType>Expansion</TemplateType>
    </Header>
    <Variables>
      <Variable name="vmProp">
        <Default>MyProp</Default>
        <_ToolTip>Property on the view model</_ToolTip>
      </Variable>
      <Variable name="viewProp">
        <Default>MyProp</Default>
        <_ToolTip>Property on the view</_ToolTip>
      </Variable>
    </Variables>
    <Code><![CDATA[this.Bind(ViewModel, vm => vm.$vmProp$, v => v.$viewProp$);]]></Code>
  </CodeTemplate>
</CodeTemplates>