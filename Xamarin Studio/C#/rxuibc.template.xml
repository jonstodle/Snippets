<?xml version="1.0" encoding="utf-8"?>
<CodeTemplates version="3.0">
  <CodeTemplate version="2.0">
    <Header>
      <_Group>C#</_Group>
      <Version />
      <MimeType>text/x-csharp</MimeType>
      <Shortcut>rxuibc</Shortcut>
      <_Description>Inserts an RxUI BindCommand</_Description>
      <TemplateType>Expansion</TemplateType>
    </Header>
    <Variables>
      <Variable name="command">
        <Default>MyCommand</Default>
        <_ToolTip>Command on the view model</_ToolTip>
      </Variable>
      <Variable name="control">
        <Default>MyControl</Default>
        <_ToolTip>Control on the view</_ToolTip>
      </Variable>
    </Variables>
    <Code><![CDATA[this.BindCommand(ViewModel, vm => vm.$command$, v => v.$control$);]]></Code>
  </CodeTemplate>
